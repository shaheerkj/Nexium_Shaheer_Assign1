{
  "compilerOptions": {
    "baseUrl": ".", // Add this line if it's not there
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["**/*.js", "**/*.jsx"], // Ensure .jsx is included if it's not already
  "exclude": ["node_modules"]
}